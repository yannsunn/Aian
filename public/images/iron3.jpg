<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="partitionGrad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#f1f5f9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="partitionIronGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="25%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="75%" style="stop-color:#475569;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#64748b;stop-opacity:1" />
    </linearGradient>
    <filter id="partitionShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="8" dy="12" stdDeviation="10" flood-color="#000000" flood-opacity="0.15"/>
    </filter>
    <filter id="glow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur stdDeviation="3"/>
      <feOffset dx="0" dy="0"/>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="800" fill="url(#partitionGrad1)"/>
  
  <!-- Architectural Partition with Geometric Design -->
  <g transform="translate(200,50)" filter="url(#partitionShadow)">
    
    <!-- Main frame structure -->
    <rect x="0" y="0" width="20" height="700" fill="url(#partitionIronGrad)" rx="3"/>
    <rect x="780" y="0" width="20" height="700" fill="url(#partitionIronGrad)" rx="3"/>
    <rect x="0" y="0" width="800" height="20" fill="url(#partitionIronGrad)" rx="3"/>
    <rect x="0" y="680" width="800" height="20" fill="url(#partitionIronGrad)" rx="3"/>
    
    <!-- Central vertical divider -->
    <rect x="390" y="20" width="20" height="660" fill="url(#partitionIronGrad)" rx="3"/>
    
    <!-- Geometric pattern - Left side -->
    <g transform="translate(50,50)">
      <!-- Diamond patterns -->
      <polygon points="150,100 200,150 150,200 100,150" fill="none" stroke="url(#partitionIronGrad)" stroke-width="8"/>
      <polygon points="150,250 200,300 150,350 100,300" fill="none" stroke="url(#partitionIronGrad)" stroke-width="8"/>
      <polygon points="150,400 200,450 150,500 100,450" fill="none" stroke="url(#partitionIronGrad)" stroke-width="8"/>
      
      <!-- Intersecting circles -->
      <circle cx="150" cy="150" r="40" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <circle cx="150" cy="300" r="40" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <circle cx="150" cy="450" r="40" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      
      <!-- Connecting lines -->
      <line x1="150" y1="110" x2="150" y2="140" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="160" x2="150" y2="190" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="210" x2="150" y2="240" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="260" x2="150" y2="290" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="310" x2="150" y2="340" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="360" x2="150" y2="390" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="410" x2="150" y2="440" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="150" y1="460" x2="150" y2="490" stroke="url(#partitionIronGrad)" stroke-width="4"/>
    </g>
    
    <!-- Geometric pattern - Right side -->
    <g transform="translate(450,50)">
      <!-- Hexagonal patterns -->
      <polygon points="150,80 180,95 180,125 150,140 120,125 120,95" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <polygon points="150,180 180,195 180,225 150,240 120,225 120,195" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <polygon points="150,280 180,295 180,325 150,340 120,325 120,295" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <polygon points="150,380 180,395 180,425 150,440 120,425 120,395" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <polygon points="150,480 180,495 180,525 150,540 120,525 120,495" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      
      <!-- Triangular accents -->
      <polygon points="150,110 165,130 135,130" fill="url(#partitionIronGrad)"/>
      <polygon points="150,210 165,230 135,230" fill="url(#partitionIronGrad)"/>
      <polygon points="150,310 165,330 135,330" fill="url(#partitionIronGrad)"/>
      <polygon points="150,410 165,430 135,430" fill="url(#partitionIronGrad)"/>
      <polygon points="150,510 165,530 135,530" fill="url(#partitionIronGrad)"/>
      
      <!-- Connecting curves -->
      <path d="M150,140 Q165,160 150,180" fill="none" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <path d="M150,240 Q165,260 150,280" fill="none" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <path d="M150,340 Q165,360 150,380" fill="none" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <path d="M150,440 Q165,460 150,480" fill="none" stroke="url(#partitionIronGrad)" stroke-width="4"/>
    </g>
    
    <!-- Central decorative element -->
    <g transform="translate(390,300)">
      <!-- Art deco style centerpiece -->
      <circle cx="10" cy="50" r="60" fill="none" stroke="url(#partitionIronGrad)" stroke-width="10"/>
      <circle cx="10" cy="50" r="30" fill="none" stroke="url(#partitionIronGrad)" stroke-width="6"/>
      <polygon points="10,20 25,35 10,50 -5,35" fill="url(#partitionIronGrad)"/>
      <polygon points="10,50 25,65 10,80 -5,65" fill="url(#partitionIronGrad)"/>
      
      <!-- Radiating lines -->
      <line x1="-40" y1="50" x2="-20" y2="50" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="40" y1="50" x2="60" y2="50" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="10" y1="-10" x2="10" y2="10" stroke="url(#partitionIronGrad)" stroke-width="4"/>
      <line x1="10" y1="90" x2="10" y2="110" stroke="url(#partitionIronGrad)" stroke-width="4"/>
    </g>
    
    <!-- Metal shine highlights -->
    <rect x="2" y="2" width="6" height="696" fill="#94a3b8" opacity="0.6" rx="2"/>
    <rect x="782" y="2" width="6" height="696" fill="#94a3b8" opacity="0.6" rx="2"/>
    <rect x="392" y="22" width="6" height="656" fill="#94a3b8" opacity="0.6" rx="2"/>
    <rect x="2" y="2" width="796" height="6" fill="#94a3b8" opacity="0.6" rx="2"/>
    <rect x="2" y="682" width="796" height="6" fill="#94a3b8" opacity="0.6" rx="2"/>
  </g>
  
  <!-- Floor shadow -->
  <ellipse cx="600" cy="780" rx="400" ry="15" fill="#000000" opacity="0.1"/>
  
  <!-- Ambient lighting effect -->
  <circle cx="600" cy="400" r="300" fill="url(#partitionIronGrad)" opacity="0.02"/>
</svg>