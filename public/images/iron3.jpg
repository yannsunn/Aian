<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="firePitBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a202c;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2d3748;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4a5568;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="firePitSteel" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#2d2d2d;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#404040;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#525252;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="fireCore" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffeb3b;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#ff9800;stop-opacity:0.9" />
      <stop offset="70%" style="stop-color:#f44336;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0" />
    </radialGradient>
    <filter id="pitGlow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="0" dy="8" stdDeviation="15" flood-color="#ff5722" flood-opacity="0.4"/>
    </filter>
    <filter id="steelShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="4" dy="6" stdDeviation="6" flood-color="#000000" flood-opacity="0.4"/>
    </filter>
    <pattern id="grillePattern" patternUnits="userSpaceOnUse" width="15" height="15">
      <rect width="15" height="15" fill="none"/>
      <rect x="0" y="0" width="2" height="15" fill="#525252"/>
      <rect x="0" y="0" width="15" height="2" fill="#525252"/>
    </pattern>
  </defs>
  
  <!-- Background (Evening campsite) -->
  <rect width="1200" height="800" fill="url(#firePitBg)"/>
  
  <!-- Ground -->
  <ellipse cx="600" cy="750" rx="600" ry="50" fill="#1a202c"/>
  
  <!-- DOD Style Sukemoe Fire 2 Fire Pit -->
  <g transform="translate(400,300)" filter="url(#pitGlow)">
    
    <!-- Main fire pit body (secondary combustion chamber) -->
    <ellipse cx="200" cy="200" rx="120" ry="40" fill="url(#firePitSteel)"/>
    <rect x="80" y="160" width="240" height="80" fill="url(#firePitSteel)" rx="40"/>
    
    <!-- Top rim -->
    <ellipse cx="200" cy="160" rx="125" ry="42" fill="#404040"/>
    <ellipse cx="200" cy="155" rx="120" ry="40" fill="url(#firePitSteel)"/>
    
    <!-- Secondary combustion holes -->
    <g transform="translate(200,160)">
      <circle cx="-80" cy="0" r="4" fill="#1a1a1a"/>
      <circle cx="-60" cy="15" r="4" fill="#1a1a1a"/>
      <circle cx="-40" cy="25" r="4" fill="#1a1a1a"/>
      <circle cx="-20" cy="30" r="4" fill="#1a1a1a"/>
      <circle cx="0" cy="32" r="4" fill="#1a1a1a"/>
      <circle cx="20" cy="30" r="4" fill="#1a1a1a"/>
      <circle cx="40" cy="25" r="4" fill="#1a1a1a"/>
      <circle cx="60" cy="15" r="4" fill="#1a1a1a"/>
      <circle cx="80" cy="0" r="4" fill="#1a1a1a"/>
      
      <circle cx="-70" cy="-15" r="4" fill="#1a1a1a"/>
      <circle cx="-50" cy="-25" r="4" fill="#1a1a1a"/>
      <circle cx="-30" cy="-30" r="4" fill="#1a1a1a"/>
      <circle cx="-10" cy="-32" r="4" fill="#1a1a1a"/>
      <circle cx="10" cy="-32" r="4" fill="#1a1a1a"/>
      <circle cx="30" cy="-30" r="4" fill="#1a1a1a"/>
      <circle cx="50" cy="-25" r="4" fill="#1a1a1a"/>
      <circle cx="70" cy="-15" r="4" fill="#1a1a1a"/>
    </g>
    
    <!-- Folding legs (DOD compact design) -->
    <g transform="translate(120,240)">
      <rect x="0" y="0" width="8" height="80" fill="url(#firePitSteel)" rx="4"/>
      <rect x="152" y="0" width="8" height="80" fill="url(#firePitSteel)" rx="4"/>
      <!-- Hinge mechanisms -->
      <circle cx="4" cy="10" r="6" fill="#2d2d2d"/>
      <circle cx="156" cy="10" r="6" fill="#2d2d2d"/>
    </g>
    
    <!-- Built-in grill grate (for yakitori) -->
    <g transform="translate(200,145)">
      <ellipse cx="0" cy="0" rx="100" ry="35" fill="url(#grillePattern)" opacity="0.8"/>
      <!-- Grill handle -->
      <rect x="105" y="-5" width="25" height="10" fill="#404040" rx="5"/>
      <circle cx="132" cy="0" r="4" fill="#2d2d2d"/>
    </g>
    
    <!-- Fire inside -->
    <g transform="translate(200,170)" filter="url(#steelShadow)">
      <!-- Fire glow base -->
      <ellipse cx="0" cy="15" rx="90" ry="25" fill="url(#fireCore)" opacity="0.8"/>
      
      <!-- Burning wood logs -->
      <rect x="-60" y="20" width="120" height="12" fill="#8b4513" rx="6"/>
      <rect x="-45" y="10" width="90" height="10" fill="#a0522d" rx="5" transform="rotate(25)"/>
      <rect x="-50" y="15" width="100" height="10" fill="#654321" rx="5" transform="rotate(-20)"/>
      
      <!-- Flames (secondary combustion effect) -->
      <path d="M-40,15 Q-45,-10 -30,-20 Q-25,-5 -15,-25 Q-10,-8 0,-30 Q5,-10 15,-25 Q25,-5 30,-20 Q45,-10 40,15 Q20,25 25,35 Q0,40 -40,15" 
            fill="#ff6b35" opacity="0.9"/>
      <path d="M-35,10 Q-38,-8 -25,-15 Q-20,-2 -10,-18 Q-5,-5 5,-20 Q10,-2 20,-15 Q35,-8 35,10 Q15,20 20,30 Q0,35 -35,10" 
            fill="#ff8f50" opacity="0.8"/>
      <path d="M-25,5 Q-28,-5 -18,-10 Q-13,0 -5,-12 Q0,-2 8,-10 Q18,-5 25,5 Q12,15 15,25 Q0,30 -25,5" 
            fill="#ffb74d" opacity="0.7"/>
      <path d="M-15,0 Q-18,-3 -10,-8 Q-5,2 0,-6 Q5,0 10,-5 Q18,-2 15,8 Q8,12 10,18 Q0,20 -15,0" 
            fill="#fff176" opacity="0.6"/>
      
      <!-- Sparks and embers -->
      <circle cx="-50" cy="-30" r="1.5" fill="#ff9800" opacity="0.9">
        <animate attributeName="cy" values="-30;-50;-30" dur="3s" repeatCount="indefinite"/>
      </circle>
      <circle cx="35" cy="-25" r="1" fill="#ff5722" opacity="0.8">
        <animate attributeName="cy" values="-25;-45;-25" dur="2.5s" repeatCount="indefinite"/>
      </circle>
      <circle cx="-15" cy="-35" r="1.2" fill="#ff6b35" opacity="0.7">
        <animate attributeName="cy" values="-35;-55;-35" dur="2.8s" repeatCount="indefinite"/>
      </circle>
      <circle cx="20" cy="-40" r="0.8" fill="#ff8f50" opacity="0.8">
        <animate attributeName="cy" values="-40;-60;-40" dur="3.2s" repeatCount="indefinite"/>
      </circle>
    </g>
    
    <!-- DOD branding -->
    <rect x="150" y="90" width="100" height="20" fill="#dc2626" rx="3"/>
    <text x="200" y="105" text-anchor="middle" font-family="system-ui, sans-serif" font-size="12" fill="white" font-weight="bold">
      SUKEMOE
    </text>
  </g>
  
  <!-- Camping scene elements -->
  <!-- Camping chair silhouette -->
  <g transform="translate(150,500)">
    <rect x="0" y="0" width="60" height="80" fill="#2d3748" opacity="0.6" rx="30"/>
    <rect x="10" y="60" width="40" height="40" fill="#1a202c" opacity="0.7" rx="5"/>
    <!-- Chair legs -->
    <rect x="5" y="100" width="8" height="50" fill="#2d3748" rx="4"/>
    <rect x="47" y="100" width="8" height="50" fill="#2d3748" rx="4"/>
  </g>
  
  <!-- Cooler box -->
  <g transform="translate(950,600)">
    <rect x="0" y="0" width="80" height="50" fill="#1a202c" opacity="0.8" rx="5"/>
    <rect x="5" y="5" width="70" height="15" fill="#2d3748" opacity="0.6" rx="3"/>
    <circle cx="75" cy="25" r="3" fill="#4a5568"/>
  </g>
  
  <!-- Wood pile -->
  <g transform="translate(50,650)">
    <rect x="0" y="0" width="100" height="12" fill="#8b4513" rx="6"/>
    <rect x="10" y="15" width="90" height="12" fill="#a0522d" rx="6"/>
    <rect x="5" y="30" width="95" height="12" fill="#654321" rx="6"/>
  </g>
  
  <!-- Ground reflection of fire -->
  <ellipse cx="600" cy="750" rx="200" ry="30" fill="#ff5722" opacity="0.1"/>
</svg>