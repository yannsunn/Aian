<svg width="800" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="productBg5" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="artSteel" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#212529;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#343a40;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#495057;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#6c757d;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="copperGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#b7410e;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#d2691e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff7f50;stop-opacity:1" />
    </linearGradient>
    <filter id="prodShadow5" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="6" dy="10" stdDeviation="8" flood-color="#000000" flood-opacity="0.15"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="800" fill="url(#productBg5)"/>
  
  <!-- Wall Art Piece -->
  <g transform="translate(200,150)" filter="url(#prodShadow5)">
    
    <!-- Main frame -->
    <rect x="0" y="0" width="400" height="400" fill="url(#artSteel)" rx="10"/>
    <rect x="20" y="20" width="360" height="360" fill="#343a40" rx="5"/>
    
    <!-- Abstract geometric design -->
    <!-- Central circle -->
    <circle cx="200" cy="200" r="120" fill="none" stroke="url(#artSteel)" stroke-width="12"/>
    <circle cx="200" cy="200" r="80" fill="none" stroke="url(#copperGrad)" stroke-width="8"/>
    <circle cx="200" cy="200" r="40" fill="none" stroke="url(#artSteel)" stroke-width="6"/>
    
    <!-- Radiating lines -->
    <g transform="translate(200,200)">
      <line x1="-140" y1="0" x2="-130" y2="0" stroke="url(#artSteel)" stroke-width="8" stroke-linecap="round"/>
      <line x1="130" y1="0" x2="140" y2="0" stroke="url(#artSteel)" stroke-width="8" stroke-linecap="round"/>
      <line x1="0" y1="-140" x2="0" y2="-130" stroke="url(#artSteel)" stroke-width="8" stroke-linecap="round"/>
      <line x1="0" y1="130" x2="0" y2="140" stroke="url(#artSteel)" stroke-width="8" stroke-linecap="round"/>
      
      <!-- Diagonal lines -->
      <line x1="-100" y1="-100" x2="-90" y2="-90" stroke="url(#copperGrad)" stroke-width="6" stroke-linecap="round"/>
      <line x1="90" y1="-100" x2="100" y2="-90" stroke="url(#copperGrad)" stroke-width="6" stroke-linecap="round"/>
      <line x1="-100" y1="90" x2="-90" y2="100" stroke="url(#copperGrad)" stroke-width="6" stroke-linecap="round"/>
      <line x1="90" y1="90" x2="100" y2="100" stroke="url(#copperGrad)" stroke-width="6" stroke-linecap="round"/>
    </g>
    
    <!-- Corner decorative elements -->
    <g transform="translate(50,50)">
      <polygon points="0,30 15,0 30,30 15,15" fill="url(#artSteel)"/>
      <circle cx="15" cy="15" r="8" fill="none" stroke="url(#copperGrad)" stroke-width="3"/>
    </g>
    
    <g transform="translate(320,50)">
      <polygon points="0,0 30,0 15,30 15,15" fill="url(#artSteel)"/>
      <circle cx="15" cy="15" r="8" fill="none" stroke="url(#copperGrad)" stroke-width="3"/>
    </g>
    
    <g transform="translate(50,320)">
      <polygon points="15,0 30,30 0,30 15,15" fill="url(#artSteel)"/>
      <circle cx="15" cy="15" r="8" fill="none" stroke="url(#copperGrad)" stroke-width="3"/>
    </g>
    
    <g transform="translate(320,320)">
      <polygon points="0,0 30,15 0,30 15,15" fill="url(#artSteel)"/>
      <circle cx="15" cy="15" r="8" fill="none" stroke="url(#copperGrad)" stroke-width="3"/>
    </g>
    
    <!-- Textural details -->
    <g transform="translate(100,100)">
      <rect x="0" y="0" width="4" height="40" fill="url(#copperGrad)" rx="2"/>
      <rect x="10" y="0" width="4" height="60" fill="url(#copperGrad)" rx="2"/>
      <rect x="20" y="0" width="4" height="50" fill="url(#copperGrad)" rx="2"/>
      <rect x="30" y="0" width="4" height="45" fill="url(#copperGrad)" rx="2"/>
    </g>
    
    <g transform="translate(260,260)">
      <rect x="0" y="0" width="4" height="40" fill="url(#copperGrad)" rx="2"/>
      <rect x="10" y="0" width="4" height="60" fill="url(#copperGrad)" rx="2"/>
      <rect x="20" y="0" width="4" height="50" fill="url(#copperGrad)" rx="2"/>
      <rect x="30" y="0" width="4" height="45" fill="url(#copperGrad)" rx="2"/>
    </g>
    
    <!-- Mounting hardware -->
    <circle cx="50" cy="50" r="8" fill="url(#artSteel)"/>
    <circle cx="350" cy="50" r="8" fill="url(#artSteel)"/>
    <circle cx="50" cy="50" r="4" fill="#6c757d"/>
    <circle cx="350" cy="50" r="4" fill="#6c757d"/>
    
    <!-- Artist signature area -->
    <rect x="250" y="350" width="130" height="30" fill="#495057" rx="5"/>
    <text x="315" y="370" text-anchor="middle" font-family="serif" font-size="12" fill="#adb5bd" font-style="italic">
      Handcrafted
    </text>
    
    <!-- Metal patina effects -->
    <rect x="22" y="22" width="356" height="10" fill="#6c757d" opacity="0.3" rx="2"/>
    <rect x="22" y="22" width="10" height="356" fill="#6c757d" opacity="0.3" rx="2"/>
    <rect x="368" y="22" width="10" height="356" fill="#6c757d" opacity="0.3" rx="2"/>
    <rect x="22" y="368" width="356" height="10" fill="#6c757d" opacity="0.3" rx="2"/>
  </g>
  
  <!-- Wall mounting context -->
  <g transform="translate(150,100)">
    <rect x="0" y="0" width="500" height="20" fill="#dee2e6" rx="10"/>
    <rect x="240" y="20" width="20" height="40" fill="#adb5bd" rx="3"/>
  </g>
  
  <!-- Product label -->
  <g transform="translate(520,650)">
    <rect x="0" y="0" width="200" height="60" fill="#495057" rx="8"/>
    <text x="100" y="25" text-anchor="middle" font-family="serif" font-size="14" fill="white" font-weight="bold">
      ハンドメイド
    </text>
    <text x="100" y="45" text-anchor="middle" font-family="serif" font-size="12" fill="#adb5bd">
      ウォールアート
    </text>
  </g>
  
  <!-- Wall shadow -->
  <ellipse cx="400" cy="750" rx="300" ry="15" fill="#000000" opacity="0.1"/>
</svg>