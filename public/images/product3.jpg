<svg width="800" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="productBg3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f4f0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e8e2dc;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="vintageSteel" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2d1810;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#4a2c1a;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#6b4423;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5a2b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="brassGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#b8860b;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#daa520;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffd700;stop-opacity:1" />
    </linearGradient>
    <filter id="prodShadow3" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="10" dy="18" stdDeviation="15" flood-color="#000000" flood-opacity="0.25"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="800" fill="url(#productBg3)"/>
  
  <!-- Vintage Bar Cart -->
  <g transform="translate(200,150)" filter="url(#prodShadow3)">
    
    <!-- Main frame structure -->
    <rect x="0" y="0" width="15" height="500" fill="url(#vintageSteel)" rx="3"/>
    <rect x="385" y="0" width="15" height="500" fill="url(#vintageSteel)" rx="3"/>
    
    <!-- Top shelf -->
    <rect x="0" y="0" width="400" height="20" fill="url(#vintageSteel)" rx="5"/>
    <rect x="15" y="20" width="370" height="80" fill="#8b4513" rx="5"/>
    
    <!-- Bottom shelf -->
    <rect x="0" y="300" width="400" height="20" fill="url(#vintageSteel)" rx="5"/>
    <rect x="15" y="320" width="370" height="80" fill="#8b4513" rx="5"/>
    
    <!-- Wood grain effects -->
    <rect x="20" y="25" width="360" height="8" fill="#654321" opacity="0.4" rx="2"/>
    <rect x="20" y="45" width="360" height="4" fill="#654321" opacity="0.3" rx="1"/>
    <rect x="20" y="325" width="360" height="8" fill="#654321" opacity="0.4" rx="2"/>
    <rect x="20" y="345" width="360" height="4" fill="#654321" opacity="0.3" rx="1"/>
    
    <!-- Brass corners and details -->
    <rect x="-5" y="-5" width="25" height="25" fill="url(#brassGrad)" rx="3"/>
    <rect x="380" y="-5" width="25" height="25" fill="url(#brassGrad)" rx="3"/>
    <rect x="-5" y="300" width="25" height="25" fill="url(#brassGrad)" rx="3"/>
    <rect x="380" y="300" width="25" height="25" fill="url(#brassGrad)" rx="3"/>
    
    <!-- Wheels -->
    <circle cx="50" cy="520" r="25" fill="url(#vintageSteel)"/>
    <circle cx="50" cy="520" r="15" fill="url(#brassGrad)"/>
    <circle cx="350" cy="520" r="25" fill="url(#vintageSteel)"/>
    <circle cx="350" cy="520" r="15" fill="url(#brassGrad)"/>
    
    <!-- Handle -->
    <rect x="410" y="150" width="30" height="15" fill="url(#brassGrad)" rx="7"/>
    <rect x="425" y="120" width="15" height="60" fill="url(#brassGrad)" rx="7"/>
    
    <!-- Bar accessories on top shelf -->
    <!-- Whiskey bottles -->
    <g transform="translate(50,30)">
      <rect x="0" y="0" width="20" height="60" fill="#8b4513" rx="3"/>
      <rect x="0" y="0" width="20" height="15" fill="#2d1810" rx="3"/>
      <text x="10" y="35" text-anchor="middle" font-family="serif" font-size="6" fill="#f4e4c1">W</text>
      
      <rect x="30" y="0" width="20" height="60" fill="#654321" rx="3"/>
      <rect x="30" y="0" width="20" height="15" fill="#2d1810" rx="3"/>
      <text x="40" y="35" text-anchor="middle" font-family="serif" font-size="6" fill="#f4e4c1">B</text>
    </g>
    
    <!-- Cocktail glasses -->
    <g transform="translate(200,35)">
      <path d="M0,50 L0,30 L-15,0 L15,0 L0,30" fill="#e8f4fd" opacity="0.7"/>
      <path d="M25,50 L25,30 L10,0 L40,0 L25,30" fill="#e8f4fd" opacity="0.7"/>
      <circle cx="0" cy="52" r="3" fill="url(#brassGrad)"/>
      <circle cx="25" cy="52" r="3" fill="url(#brassGrad)"/>
    </g>
    
    <!-- Ice bucket -->
    <g transform="translate(300,25)">
      <ellipse cx="25" cy="55" rx="22" ry="12" fill="url(#brassGrad)"/>
      <rect x="3" y="20" width="44" height="35" fill="url(#brassGrad)" rx="22"/>
      <ellipse cx="25" cy="20" rx="20" ry="10" fill="#daa520"/>
      <!-- Handles -->
      <ellipse cx="5" cy="35" rx="3" ry="8" fill="none" stroke="url(#brassGrad)" stroke-width="2"/>
      <ellipse cx="45" cy="35" rx="3" ry="8" fill="none" stroke="url(#brassGrad)" stroke-width="2"/>
    </g>
    
    <!-- Wine rack on bottom shelf -->
    <g transform="translate(30,340)">
      <rect x="0" y="0" width="340" height="30" fill="url(#vintageSteel)" rx="2"/>
      <!-- Wine bottle holes -->
      <circle cx="50" cy="15" r="12" fill="#2d1810"/>
      <circle cx="100" cy="15" r="12" fill="#2d1810"/>
      <circle cx="150" cy="15" r="12" fill="#2d1810"/>
      <circle cx="200" cy="15" r="12" fill="#2d1810"/>
      <circle cx="250" cy="15" r="12" fill="#2d1810"/>
      <circle cx="300" cy="15" r="12" fill="#2d1810"/>
      
      <!-- Wine bottles -->
      <rect x="46" y="-20" width="8" height="35" fill="#2d5016" rx="4"/>
      <rect x="96" y="-20" width="8" height="35" fill="#722f37" rx="4"/>
      <rect x="196" y="-20" width="8" height="35" fill="#2d1810" rx="4"/>
    </g>
    
    <!-- Vintage nameplate -->
    <rect x="150" y="250" width="100" height="30" fill="url(#brassGrad)" rx="5"/>
    <text x="200" y="270" text-anchor="middle" font-family="serif" font-size="12" fill="#2d1810" font-weight="bold">
      BAR CART
    </text>
    
    <!-- Metal shine effects -->
    <rect x="2" y="2" width="4" height="496" fill="#8b5a2b" opacity="0.6" rx="1"/>
    <rect x="387" y="2" width="4" height="496" fill="#8b5a2b" opacity="0.6" rx="1"/>
  </g>
  
  <!-- Product label -->
  <g transform="translate(520,650)">
    <rect x="0" y="0" width="180" height="60" fill="#8b4513" rx="8"/>
    <text x="90" y="25" text-anchor="middle" font-family="serif" font-size="14" fill="#f4e4c1" font-weight="bold">
      ヴィンテージ
    </text>
    <text x="90" y="45" text-anchor="middle" font-family="serif" font-size="12" fill="#ddd6c7">
      バーカート
    </text>
  </g>
  
  <!-- Floor shadow -->
  <ellipse cx="400" cy="750" rx="300" ry="25" fill="#000000" opacity="0.15"/>
</svg>