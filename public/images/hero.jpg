      <stop offset="0%" style="stop-color:#0c0c0c;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#2d2d2d;stop-opacity:1" />
