<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="irisRackBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#f1f5f9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="steelRackGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#475569;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#64748b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#94a3b8;stop-opacity:1" />
    </linearGradient>
    <filter id="rackShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="6" dy="10" stdDeviation="8" flood-color="#000000" flood-opacity="0.12"/>
    </filter>
    <pattern id="meshPattern" patternUnits="userSpaceOnUse" width="20" height="20">
      <rect width="20" height="20" fill="none"/>
      <rect x="0" y="0" width="18" height="18" fill="none" stroke="#64748b" stroke-width="1"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="800" fill="url(#irisRackBg)"/>
  
  <!-- IRIS OHYAMA Style Steel Rack -->
  <g transform="translate(250,100)" filter="url(#rackShadow)">
    
    <!-- Main vertical posts (Chrome finish style) -->
    <rect x="0" y="0" width="20" height="600" fill="url(#steelRackGrad)" rx="10"/>
    <rect x="680" y="0" width="20" height="600" fill="url(#steelRackGrad)" rx="10"/>
    
    <!-- Chrome highlights on posts -->
    <rect x="3" y="3" width="8" height="594" fill="#cbd5e1" opacity="0.8" rx="4"/>
    <rect x="683" y="3" width="8" height="594" fill="#cbd5e1" opacity="0.8" rx="4"/>
    
    <!-- Shelf frames (5 levels) -->
    <!-- Top shelf -->
    <rect x="-15" y="0" width="730" height="15" fill="url(#steelRackGrad)" rx="3"/>
    <rect x="-15" y="15" width="730" height="3" fill="#cbd5e1" opacity="0.6"/>
    
    <!-- Second shelf -->
    <rect x="-15" y="140" width="730" height="15" fill="url(#steelRackGrad)" rx="3"/>
    <rect x="-15" y="155" width="730" height="3" fill="#cbd5e1" opacity="0.6"/>
    
    <!-- Third shelf -->
    <rect x="-15" y="280" width="730" height="15" fill="url(#steelRackGrad)" rx="3"/>
    <rect x="-15" y="295" width="730" height="3" fill="#cbd5e1" opacity="0.6"/>
    
    <!-- Fourth shelf -->
    <rect x="-15" y="420" width="730" height="15" fill="url(#steelRackGrad)" rx="3"/>
    <rect x="-15" y="435" width="730" height="3" fill="#cbd5e1" opacity="0.6"/>
    
    <!-- Bottom shelf -->
    <rect x="-15" y="585" width="730" height="15" fill="url(#steelRackGrad)" rx="3"/>
    <rect x="-15" y="600" width="730" height="3" fill="#cbd5e1" opacity="0.6"/>
    
    <!-- Wire mesh shelves (IRIS OHYAMA signature feature) -->
    <rect x="20" y="18" width="660" height="120" fill="url(#meshPattern)" opacity="0.4"/>
    <rect x="20" y="158" width="660" height="120" fill="url(#meshPattern)" opacity="0.4"/>
    <rect x="20" y="298" width="660" height="120" fill="url(#meshPattern)" opacity="0.4"/>
    <rect x="20" y="438" width="660" height="120" fill="url(#meshPattern)" opacity="0.4"/>
    
    <!-- Adjustable brackets (IRIS OHYAMA style) -->
    <g transform="translate(0,140)">
      <rect x="-5" y="-10" width="10" height="20" fill="#475569" rx="2"/>
      <rect x="695" y="-10" width="10" height="20" fill="#475569" rx="2"/>
    </g>
    <g transform="translate(0,280)">
      <rect x="-5" y="-10" width="10" height="20" fill="#475569" rx="2"/>
      <rect x="695" y="-10" width="10" height="20" fill="#475569" rx="2"/>
    </g>
    <g transform="translate(0,420)">
      <rect x="-5" y="-10" width="10" height="20" fill="#475569" rx="2"/>
      <rect x="695" y="-10" width="10" height="20" fill="#475569" rx="2"/>
    </g>
    
    <!-- Storage items on shelves -->
    <!-- Books on second shelf -->
    <g transform="translate(50,155)">
      <rect x="0" y="5" width="12" height="90" fill="#ef4444" rx="2"/>
      <rect x="15" y="5" width="12" height="90" fill="#3b82f6" rx="2"/>
      <rect x="30" y="5" width="12" height="90" fill="#10b981" rx="2"/>
      <rect x="45" y="5" width="12" height="90" fill="#f59e0b" rx="2"/>
      <rect x="60" y="5" width="12" height="90" fill="#8b5cf6" rx="2"/>
    </g>
    
    <!-- Storage boxes on third shelf -->
    <g transform="translate(200,295)">
      <rect x="0" y="5" width="80" height="60" fill="#6b7280" rx="4"/>
      <rect x="90" y="5" width="80" height="60" fill="#6b7280" rx="4"/>
      <rect x="180" y="5" width="80" height="60" fill="#6b7280" rx="4"/>
      <!-- Box labels -->
      <rect x="10" y="15" width="60" height="15" fill="#f3f4f6" rx="2"/>
      <rect x="100" y="15" width="60" height="15" fill="#f3f4f6" rx="2"/>
      <rect x="190" y="15" width="60" height="15" fill="#f3f4f6" rx="2"/>
    </g>
    
    <!-- Kitchen items on fourth shelf -->
    <g transform="translate(450,435)">
      <!-- Pot -->
      <ellipse cx="30" cy="75" rx="25" ry="12" fill="#374151"/>
      <rect x="5" y="35" width="50" height="40" fill="#4b5563" rx="25"/>
      <rect x="52" y="50" width="15" height="4" fill="#4b5563" rx="2"/>
      
      <!-- Plates stack -->
      <ellipse cx="100" cy="75" rx="20" ry="8" fill="#f8fafc"/>
      <ellipse cx="100" cy="72" rx="20" ry="8" fill="#f1f5f9"/>
      <ellipse cx="100" cy="69" rx="20" ry="8" fill="#e2e8f0"/>
    </g>
  </g>
  
  <!-- IRIS OHYAMA style branding area -->
  <g transform="translate(50,650)">
    <rect x="0" y="0" width="120" height="40" fill="#1e40af" rx="5"/>
    <text x="60" y="25" text-anchor="middle" font-family="system-ui, sans-serif" font-size="12" fill="white" font-weight="bold">
      PREMIUM STEEL
    </text>
  </g>
  
  <!-- Floor reflection -->
  <ellipse cx="600" cy="750" rx="400" ry="25" fill="#000000" opacity="0.08"/>
</svg>