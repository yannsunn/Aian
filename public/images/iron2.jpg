<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="tableGrad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fefefe;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#f1f5f9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="tableWoodGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#92400e;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#a16207;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#b45309;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d97706;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="tableIronGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#475569;stop-opacity:1" />
    </linearGradient>
    <filter id="tableShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="12" dy="16" stdDeviation="12" flood-color="#000000" flood-opacity="0.2"/>
    </filter>
    <pattern id="woodGrain" patternUnits="userSpaceOnUse" width="8" height="4">
      <rect width="8" height="4" fill="#a16207"/>
      <rect x="0" y="0" width="8" height="1" fill="#92400e" opacity="0.3"/>
      <rect x="0" y="2" width="8" height="1" fill="#b45309" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="800" fill="url(#tableGrad1)"/>
  
  <!-- Contemporary Dining Table -->
  <g transform="translate(150,200)" filter="url(#tableShadow)">
    
    <!-- Table Top - Premium Oak -->
    <rect x="0" y="300" width="900" height="50" fill="url(#tableWoodGrad)" rx="8"/>
    <rect x="0" y="300" width="900" height="50" fill="url(#woodGrain)" opacity="0.4" rx="8"/>
    
    <!-- Wood grain highlights -->
    <rect x="10" y="305" width="880" height="8" fill="#f59e0b" opacity="0.3" rx="4"/>
    <rect x="10" y="337" width="880" height="8" fill="#78350f" opacity="0.3" rx="4"/>
    
    <!-- Table Base - Black Steel Frame -->
    <!-- Horizontal base beam -->
    <rect x="50" y="350" width="800" height="15" fill="url(#tableIronGrad)" rx="3"/>
    
    <!-- Vertical support posts -->
    <rect x="100" y="350" width="15" height="150" fill="url(#tableIronGrad)" rx="3"/>
    <rect x="785" y="350" width="15" height="150" fill="url(#tableIronGrad)" rx="3"/>
    
    <!-- Cross braces -->
    <rect x="115" y="420" width="670" height="12" fill="url(#tableIronGrad)" rx="3"/>
    
    <!-- Feet -->
    <ellipse cx="107.5" cy="515" rx="25" ry="8" fill="url(#tableIronGrad)"/>
    <ellipse cx="792.5" cy="515" rx="25" ry="8" fill="url(#tableIronGrad)"/>
    
    <!-- Metal highlights and details -->
    <rect x="52" y="352" width="796" height="3" fill="#64748b" opacity="0.8" rx="1"/>
    <rect x="102" y="352" width="3" height="146" fill="#64748b" opacity="0.8" rx="1"/>
    <rect x="787" y="352" width="3" height="146" fill="#64748b" opacity="0.8" rx="1"/>
    <rect x="117" y="422" width="666" height="3" fill="#64748b" opacity="0.8" rx="1"/>
    
    <!-- Bolts and joints -->
    <circle cx="115" cy="357" r="4" fill="#1e293b"/>
    <circle cx="785" cy="357" r="4" fill="#1e293b"/>
    <circle cx="115" cy="426" r="4" fill="#1e293b"/>
    <circle cx="785" cy="426" r="4" fill="#1e293b"/>
    
    <!-- Subtle rivets -->
    <circle cx="115" cy="357" r="2" fill="#94a3b8"/>
    <circle cx="785" cy="357" r="2" fill="#94a3b8"/>
    <circle cx="115" cy="426" r="2" fill="#94a3b8"/>
    <circle cx="785" cy="426" r="2" fill="#94a3b8"/>
  </g>
  
  <!-- Table setting elements -->
  <!-- Place settings -->
  <g transform="translate(300,480)">
    <!-- Plate -->
    <circle cx="0" cy="0" r="25" fill="#f8fafc" stroke="#e2e8f0" stroke-width="2"/>
    <!-- Cutlery -->
    <rect x="-40" y="-2" width="30" height="2" fill="#64748b" rx="1"/>
    <rect x="15" y="-2" width="30" height="2" fill="#64748b" rx="1"/>
  </g>
  
  <g transform="translate(600,480)">
    <!-- Plate -->
    <circle cx="0" cy="0" r="25" fill="#f8fafc" stroke="#e2e8f0" stroke-width="2"/>
    <!-- Cutlery -->
    <rect x="-40" y="-2" width="30" height="2" fill="#64748b" rx="1"/>
    <rect x="15" y="-2" width="30" height="2" fill="#64748b" rx="1"/>
  </g>
  
  <!-- Decorative vase centerpiece -->
  <g transform="translate(600,460)">
    <ellipse cx="0" cy="25" rx="20" ry="8" fill="#475569"/>
    <rect x="-15" y="0" width="30" height="25" fill="#6b7280" rx="15"/>
    <ellipse cx="0" cy="0" rx="12" ry="6" fill="#374151"/>
    <!-- Flowers -->
    <circle cx="-5" cy="-8" r="3" fill="#dc2626"/>
    <circle cx="5" cy="-10" r="3" fill="#dc2626"/>
    <circle cx="0" cy="-12" r="3" fill="#dc2626"/>
  </g>
  
  <!-- Floor shadow -->
  <ellipse cx="600" cy="750" rx="500" ry="30" fill="#000000" opacity="0.08"/>
</svg>