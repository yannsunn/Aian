<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="dodTableBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#334155;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="fireTableGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#18181b;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#27272a;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#3f3f46;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#52525b;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="fireGlow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ff7b00;stop-opacity:0.8" />
      <stop offset="30%" style="stop-color:#ff5722;stop-opacity:0.6" />
      <stop offset="70%" style="stop-color:#d32f2f;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0" />
    </radialGradient>
    <filter id="tableGlow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="8" dy="12" stdDeviation="12" flood-color="#ff5722" flood-opacity="0.3"/>
    </filter>
    <filter id="fireShadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="4" dy="8" stdDeviation="8" flood-color="#000000" flood-opacity="0.5"/>
    </filter>
  </defs>
  
  <!-- Background (Outdoor camping scene) -->
  <rect width="1200" height="800" fill="url(#dodTableBg)"/>
  
  <!-- Stars in background -->
  <circle cx="150" cy="80" r="1" fill="white" opacity="0.8"/>
  <circle cx="300" cy="120" r="1.5" fill="white" opacity="0.9"/>
  <circle cx="850" cy="90" r="1" fill="white" opacity="0.7"/>
  <circle cx="1050" cy="140" r="1" fill="white" opacity="0.8"/>
  <circle cx="200" cy="160" r="0.5" fill="white" opacity="0.6"/>
  
  <!-- DOD Style Tequila Fire Table -->
  <g transform="translate(300,350)" filter="url(#tableGlow)">
    
    <!-- Fire pit base (center) -->
    <ellipse cx="300" cy="200" rx="80" ry="25" fill="url(#fireTableGrad)"/>
    <rect x="220" y="175" width="160" height="50" fill="url(#fireTableGrad)" rx="25"/>
    
    <!-- Table plates (3mm iron - DOD signature) -->
    <!-- Left plate -->
    <polygon points="0,150 200,130 200,180 0,200" fill="url(#fireTableGrad)"/>
    <polygon points="0,150 200,130 200,135 0,155" fill="#71717a" opacity="0.8"/>
    
    <!-- Right plate -->
    <polygon points="400,130 600,150 600,200 400,180" fill="url(#fireTableGrad)"/>
    <polygon points="400,130 600,150 600,155 400,135" fill="#71717a" opacity="0.8"/>
    
    <!-- Table legs (adjustable DOD style) -->
    <!-- Left legs -->
    <rect x="30" y="200" width="12" height="120" fill="url(#fireTableGrad)" rx="6"/>
    <rect x="170" y="200" width="12" height="120" fill="url(#fireTableGrad)" rx="6"/>
    <!-- Right legs -->
    <rect x="418" y="200" width="12" height="120" fill="url(#fireTableGrad)" rx="6"/>
    <rect x="558" y="200" width="12" height="120" fill="url(#fireTableGrad)" rx="6"/>
    
    <!-- Leg adjustment mechanisms -->
    <circle cx="36" cy="280" r="8" fill="#52525b"/>
    <circle cx="176" cy="280" r="8" fill="#52525b"/>
    <circle cx="424" cy="280" r="8" fill="#52525b"/>
    <circle cx="564" cy="280" r="8" fill="#52525b"/>
    
    <!-- Fire in the center -->
    <g transform="translate(300,175)" filter="url(#fireShadow)">
      <!-- Fire glow -->
      <circle cx="0" cy="10" r="60" fill="url(#fireGlow)"/>
      
      <!-- Wood logs -->
      <rect x="-40" y="20" width="80" height="8" fill="#8b4513" rx="4"/>
      <rect x="-30" y="10" width="60" height="8" fill="#a0522d" rx="4" transform="rotate(30)"/>
      <rect x="-35" y="15" width="70" height="8" fill="#8b4513" rx="4" transform="rotate(-20)"/>
      
      <!-- Flames -->
      <path d="M-20,20 Q-25,0 -15,-10 Q-10,5 -5,-15 Q0,0 5,-12 Q10,8 15,-8 Q20,2 25,-5 Q15,15 20,25 Q0,30 -20,20" 
            fill="#ff6b35" opacity="0.9"/>
      <path d="M-15,15 Q-18,-5 -8,-12 Q-3,2 2,-10 Q7,5 12,-5 Q17,0 20,8 Q10,20 15,25 Q0,25 -15,15" 
            fill="#ff8f50" opacity="0.8"/>
      <path d="M-10,10 Q-12,-3 -5,-8 Q0,0 5,-6 Q10,2 15,5 Q8,15 10,20 Q0,20 -10,10" 
            fill="#ffb74d" opacity="0.7"/>
      
      <!-- Sparks -->
      <circle cx="-30" cy="-20" r="1" fill="#ff9800" opacity="0.8"/>
      <circle cx="25" cy="-15" r="1.5" fill="#ff5722" opacity="0.9"/>
      <circle cx="-10" cy="-25" r="0.8" fill="#ff6b35" opacity="0.7"/>
      <circle cx="15" cy="-30" r="1.2" fill="#ff8f50" opacity="0.8"/>
    </g>
    
    <!-- Cooking items on table -->
    <!-- Dutch oven on left plate -->
    <g transform="translate(80,140)">
      <ellipse cx="0" cy="25" rx="30" ry="12" fill="#1f2937"/>
      <rect x="-30" y="0" width="60" height="25" fill="#374151" rx="30"/>
      <circle cx="-35" cy="12" r="3" fill="#4b5563"/>
      <rect x="-38" y="10" width="15" height="4" fill="#4b5563" rx="2"/>
      <!-- Steam -->
      <path d="M-10,-5 Q-8,-15 -6,-8 Q-4,-18 -2,-10 Q0,-16 2,-8" 
            fill="none" stroke="white" stroke-width="1" opacity="0.6"/>
    </g>
    
    <!-- Camping mug on right plate -->
    <g transform="translate(480,135)">
      <ellipse cx="0" cy="20" rx="15" ry="8" fill="#6b7280"/>
      <rect x="-15" y="0" width="30" height="20" fill="#9ca3af" rx="15"/>
      <rect x="12" y="8" width="8" height="4" fill="#9ca3af" rx="2"/>
    </g>
    
    <!-- DOD logo area -->
    <rect x="250" y="90" width="100" height="25" fill="#dc2626" rx="5"/>
    <text x="300" y="107" text-anchor="middle" font-family="system-ui, sans-serif" font-size="14" fill="white" font-weight="bold">
      TEQUILA
    </text>
  </g>
  
  <!-- Ground/camping environment -->
  <ellipse cx="600" cy="750" rx="500" ry="30" fill="#000000" opacity="0.4"/>
  
  <!-- Tent silhouette in background -->
  <g transform="translate(900,500)">
    <polygon points="0,100 50,0 100,100" fill="#1e293b" opacity="0.6"/>
    <line x1="0" y1="100" x2="100" y2="100" stroke="#334155" stroke-width="2"/>
  </g>
  
  <!-- Camping gear silhouettes -->
  <g transform="translate(100,600)">
    <!-- Backpack -->
    <rect x="0" y="0" width="30" height="50" fill="#1e293b" opacity="0.5" rx="5"/>
    <rect x="5" y="10" width="20" height="15" fill="#334155" opacity="0.4" rx="3"/>
  </g>
</svg>